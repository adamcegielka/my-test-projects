{
  "name": "automationexercise-playwright",
  "version": "1.0.0",
  "description": "The test project involves running automated tests using a ready-made test Scenario and API testing in Automation.",
  "main": "index.js",
  "scripts": {
    "test:record": "npx playwright codegen https://automationexercise.com/",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "report": "npx playwright show-report",
    "open:ui": "npx playwright test --ui",
    "test:api": "playwright test --config=api.config.ts --project=Chromium"
  },
  "keywords": [],
  "author": "Adam Cegie≈Çka",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.35.0",
    "@types/jest": "^29.5.1",
    "prettier": "2.8.8"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0"
  }
}
